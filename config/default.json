{
  "COMMON": {
    "log" : {
      "fileName" : "api-error-logs.log",
      "logsDirectory" : "logs"
    },
    "success": "Proceso ejecutado satisfactoriamente"
  },
  "PATH": {
  	"app": "D:/tempo/app",
  	"model": "D:/tempo/app/model",
  	"store": "D:/tempo/app/store",
  	"service": "D:/tempo/app/service",
  	"view": "D:/tempo/app/view",
  	"controller": "D:/tempo/app/controller",
  	"context": "D:/tempo/app/context"
  },
  "WORKSPACE": {
  	"app": "app",
  	"model": "app.model",
  	"store": "app.store",
  	"service": "app.service",
  	"view": "app.view",
  	"controller": "app.controller",
  	"context": "app.context"
  },
  "CLASS_EXTJS": {
    "controller": "Deft.mvc.ViewController",
  	"model": "Ext.data.Model",
  	"panel": "Ext.panel.Panel",
    "form": "Ext.form.Panel"
  },
  "CLASS_ABSTRACT": {
    "controller": "app.controller.abstract.AbstractController",
    "gridController": "app.controller.abstract.AbstractGridController",
    "formController": "app.controller.abstract.AbstractFormController",
    "store": "app.store.abstract.AbstractStore",
    "context": "app.context.abstract.AbstractContext",
    "grid": "app.view.abstract.AbstractGrid",
    "form": "app.view.abstract.AbstractForm",
    "filterWindow": "app.view.abstract.AbstractFilterWindow"
  },
  "TOKEN": {
    "key": "appToken"
  },
  "PROXY_STORE": {
    "type": "ajax",
    "reader": {
      "type": "json",
      "root": "rows",
      "totalProperty": "total",
      "successProperty": "success",
      "messageProperty": "msg"
    },
    "writer": {
      "type": "json",
      "writeAllFields": true,
      "allowSingle": false,
      "root": "data"
    },
    "sorters": {
      "direction": "DESC"
    }
  },
  "WIDTH": {
  	"panel": {
  		"region": {
  			"center": "60%",
  			"east": "40%",
  			"west": "20%"
  		}
  	}
  },
  "PARAMS": {
    "queryParam": "contains"
  }
}